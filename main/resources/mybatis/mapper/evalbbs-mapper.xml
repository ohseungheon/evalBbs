<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.evalBbs.dao.IEvalBbsDao">
	
	
	<select id="getList" resultType="com.green.evalBbs.dto.EvalBbsDto">
		SELECT * FROM evalbbs
	</select>
	<select id="getDto">
		SELECT * FROM simpleBBS Where title = #{title}
	</select>
	<insert id="write">
		INSERT INTO evalbbs VALUES(null,#{dto.title},#{dto.content},#{dto.writer},#{dto.regdate})
	</insert>
</mapper>